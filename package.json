{
  "name": "torchbearer",
  "description": "Beramarket Indexer using Envio",
  "version": "0.1.2",
  "private": true,
  "author": "Johannes Krauser III <detroitmetalcrypto@gmail.com",
  "scripts": {
    "build": "tsc --build",
    "generate": "ts-node scripts/generateSchema.ts && ts-node scripts/generateSchemaTypes.ts",
    "codegen": "pnpm run generate && envio codegen",
    "check:license": "pipx run reuse lint",
    "check:types": "tsc --noEmit --pretty",
    "clean": "tsc --clean",
    "dev": "pnpm run generate && envio dev",
    "lint:production": "NODE_ENV=production eslint . --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "prepare": "husky install",
    "start": "ts-node generated/src/Index.bs.js",
    "stop": "envio stop",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "chai": "4.3.10",
    "envio": "2.3.0",
    "ethers": "6.8.0",
    "viem": "2.13.7",
    "decimal.js": "10.4.3",
    "amqplib": "^0.10.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@eslint-types/import": "^2.29.1",
    "@eslint-types/prettier": "^5.1.3",
    "@eslint-types/typescript-eslint": "^7.5.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@types/amqplib": "^0.10.3",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.4",
    "inquirer": "^9.3.6",
    "lint-staged": "^15.2.9",
    "mocha": "10.2.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "optionalDependencies": {
    "generated": "./generated"
  }
}
